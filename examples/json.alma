-file    = json .
-json    = object | array | bool | null | string | number
-object  = '{'< . \members? . '}'
-members = member ( . ',' . member)*
-member  = name . ':' . json
-array   = '['< . \elements? . ']'
-elements= json ( . ',' . json)*
-string  = '"' ~ '"'
-number  = ['-+']? \9* ('.' \9*)?
-bool    = 'true' | 'false'
-null    = 'null'
-name    = '"' \text '"'
-text    = [zZ9 '_-']+
-