document = doctype?, root
root = node
doctype = '<?xml' attributes? , '?>'
node = cdata | comment | element | text
element = '<' name attributes? , '>', nodes?, '</' name '>'
nodes = node (, node)*
attributes = ~ attribute (~ attribute)*
attribute = name '=' '"' value '"'
text = !'<'*
value = ..
cdata = '<![CDATA[' data ']]>'
data = ..
comment = '<!--' info '-->'
info = ..
name = @+