document = doctype root
root = node
doctype = '<?xml' attribute* '?>'
node = element | comment | cdata | text 
element = open attribute* '>' node* close
[] open = '<' name
[] close = '</' name '>'
[] attribute = name '=' '"' value '"'
text = !'<'
value = !'"'
cdata = '<![CDATA[' data ']]>'
data = ..
comment = '<!--' info '-->'
info = ..