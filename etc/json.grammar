json ::= object | array | bool | null | string | number
object ::= '{', members?, '}'
members ::= member (, ',', member)*
member ::= name, ':', json
array ::= '[', elements?, ']'
elements ::= json (, ',', json)*
string ::= '"' .. '"'
number ::= #* ['.' #*]
bool ::= 'true' | 'false'
null ::= 'null'
name ::= '"' text '"'
text ::= { @ # '_' '-' }+