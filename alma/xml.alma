-document   = doctype? . node@root
-doctype    = '<?xml' attributes? . '?>'
-node       = cdata | comment | element | text
-element    = '<' name attributes? . '>' . nodes? . '</' name '>'
-nodes      = node (. node)*
-attributes = (: attribute)+
-attribute  = name '=' '"' ~ '"'
-text       = ['<']^*
-cdata      = '<![CDATA[' ~ ']]>'
-comment    = '<!--' ~ '-->'
-name       = [zZ]+